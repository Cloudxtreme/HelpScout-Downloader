{
    "name": "graham-campbell/helpscout-downloader",
    "description": "HelpScout Downloader Is A CLI Tool To Download Data From HelpScout",
    "keywords": ["helpscout", "downloader", "HelpScout", "HelpScout Downloader", "HelpScout-Downloader", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "bcncommerce/json-stream": "^0.3",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\HelpScout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\HelpScout\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        },
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": [
        "stats"
    ]
}
